{
    "name": "kerkness/wc-hub",
    "description": "Integrates WooCommerce and Hubspot.",
    "keywords": ["wordpress", "plugin", "woocommerce", "hubspot"],
    "homepage": "https://kerkness.ca",
    "license": "GNU",
    "authors": [
      {
        "name": "kerkness",
        "email": "ryan@kerkness.ca",
        "homepage": "https://kerkness.ca"
      }
    ],
    "type": "wordpress-plugin",
    "require": {
      "php": ">=7.1.0",
      "composer/installers": "v1.0.6",
        "hubspot/api-client": "^8.3"
    },
    "autoload"    : {
      "psr-4" : {
        "WCHub\\" : "src"
      }
    },
    "config": { 
      "vendor-dir": "lib",
        "allow-plugins": {
            "composer/installers": true
        }
    }
  }
